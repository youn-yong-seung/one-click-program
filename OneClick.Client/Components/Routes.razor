<Router AppAssembly="@typeof(Routes).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(OneClick.Client.Shared.DashboardLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(OneClick.Client.Shared.DashboardLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
